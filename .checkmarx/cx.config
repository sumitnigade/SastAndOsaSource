project: 
  fullPath: "SNProject30"
  origin: "jenkins"
sast: 
  configuration: "Korean"
  excludeFolders: \"_cvs, .svn, .hg, .git, .bzr, bin, obj, backup, node_modules\"
  includeExcludePattern: \"!**/*.DS_Store, **/*.java\"
  high: 3
  medium: 1
  low: 2
  incremental: true
  preset: "Android"
  privateScan: false
sca:
  fileInclude: ".zip" 
  fileExclude: ".jar" 
  pathExclude: "test"
  high: 3
  medium: 3
  low: 3
